{
    "name": "wappsto-wapp",
    "version": "0.7.6",
    "license": "MIT",
    "author": "Seluxit A/S",
    "main": "dist/wappsto-wapp.cjs.production.min.js",
    "module": "dist/wappsto-wapp.esm.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist/wappsto-wapp.esm.js",
        "dist/wappsto-wapp.cjs.production.min.js",
        "dist/wappsto-wapp.js",
        "dist/*.d.ts",
        "dist/**/*.d.ts"
    ],
    "keywords": [
        "wappsto",
        "wapp",
        "api",
        "iot",
        "seluxit"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Wappsto/javascript-wappsto-wapp"
    },
    "scripts": {
        "analyze": "size-limit --why",
        "build": "dts build",
        "postbuild": "yarn browserify dist/wappsto-wapp.cjs.production.min.js --s Wappsto -o dist/wappsto-wapp.js",
        "lint": "dts lint src test",
        "lint:fix": "yarn lint --fix",
        "prepare": "yarn build",
        "size": "size-limit",
        "start": "dts watch",
        "test": "dts test --coverage",
        "generate-types": "ts-interface-builder -v -g -c src/util/interfaces.ts; yarn lint:fix",
        "prepublish": "dts build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "dts lint && dts test && dts build"
        }
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "jest": {
        "testEnvironment": "node"
    },
    "peerDependencies": {},
    "engines": {
        "node": ">=12"
    },
    "size-limit": [
        {
            "path": "dist/wappsto-wapp.cjs.production.min.js",
            "limit": "70 KB"
        },
        {
            "path": "dist/wappsto-wapp.esm.js",
            "limit": "50 KB"
        }
    ],
    "dependencies": {
        "axios": "^0.25.0",
        "class-transformer": "^0.5.1",
        "isomorphic-ws": "^4.0.1",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "reflect-metadata": "^0.1.13",
        "ts-interface-checker": "^1.0.2",
        "universal-websocket-client": "^1.0.2"
    },
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "@tsconfig/recommended": "^1.0.1",
        "@types/lodash": "^4.14.178",
        "@types/ws": "^8.2.2",
        "browserify": "^17.0.0",
        "dts-cli": "^1.2.0",
        "husky": "^7.0.4",
        "jest-websocket-mock": "^2.2.1",
        "size-limit": "^7.0.8",
        "ts-interface-builder": "^0.3.3",
        "tslib": "^2.3.1",
        "typescript": "^4.5.5"
    }
}
