{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --coverage",
    "lint": "tsdx lint",
    "fix-lint": "tsdx lint --fix",
    "prepare": "tsdx build; npx browserify dist/wappsto-wapp.cjs.production.min.js --s wapp -o dist/wappsto-wapp.js",
    "doc": "typedoc --exclude **/*.test.ts src/index.ts",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "wappsto-wapp",
  "author": "Andreas Bomholtz",
  "module": "dist/wappsto-wapp.esm.js",
  "size-limit": [
    {
      "path": "dist/wappsto-wapp.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/wappsto-wapp.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "browserify": "^17.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.176",
    "@types/websocket": "^1.0.4",
    "axios": "^0.24.0",
    "class-transformer": "^0.4.0",
    "reflect-metadata": "^0.1.13",
    "websocket": "^1.0.34"
  }
}
